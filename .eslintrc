{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
        "semi": 1,
        "no-console": "warn",
        "no-debugger": "warn",
        "no-unused-vars": "off"
    }
}